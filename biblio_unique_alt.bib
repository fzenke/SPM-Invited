number		= {5},
pages		= {3524--3534},
pmid		= {8930290},
title		= {Analysis of dynamic spectra in ferret primary auditory
		  cortex. {II}. {P}rediction of unit responses to arbitrary
		  dynamic spectra.},
volume		= {76},
